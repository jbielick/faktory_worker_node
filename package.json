{
  "name": "faktory-worker",
  "version": "0.9.1",
  "description": "A faktory worker framework for node apps",
  "main": "index.js",
  "author": "Josh Bielick <jbielick@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "test": "nyc --cache ava test/**/*.test.js",
    "coverage": "nyc report --reporter=html",
    "lint": "eslint lib/**/*.js"
  },
  "keywords": [
    "faktory",
    "client",
    "node",
    "server",
    "job",
    "background",
    "async"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jbielick/faktory_worker_node"
  },
  "bin": {
    "faktory-work": "bin/faktory-worker",
    "faktory-cluster": "bin/faktory-cluster"
  },
  "ava": {
    "verbose": true,
    "timeout": "10s"
  },
  "engines": {
    "node": ">=7"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nyc": "^11.2.1"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "debug": "^3.1.0",
    "faktory-client": "^0.6.1",
    "generic-pool": "^3.4.2",
    "koa-compose": "^4.0.0",
    "uuid": "^3.1.0"
  }
}
